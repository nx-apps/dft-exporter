{
  "entrypoint": "index.html",
  "shell": "src/nylon-app.html",
  "fragments": [
    "src/pages/page-401/page-401.html",
    "src/pages/page-404/page-404.html",
    "src/pages/page-draft/page-draft.html",
    "src/pages/page-admin-exporter/page-admin-exporter.html",
    "src/pages/page-setting/page-setting.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json",
    "fonts/**/*",
    "bower_components/gl-font/**/*"
  ],
  "extraDependencies": [
    "bundle.js",
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.min.js"
  ],
  "builds": [
    {
      "name": "default",
      "bundle": false,
      "html": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "js": {
        "minify": true,
        "compile": false
      },
      "addServiceWorker": false,
      "addPushManifest": false
    }
  ],
  "lint": {
    "rules": ["polymer-2-hybrid"]
  }
}